{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:protractor/recommended",
    "plugin:jasmine/recommended"
  ],
  "rules": {
    "comma-dangle": 0,
    "indent": 2,
    "object-shorthand": 0,
    "func-names": 0,
    "max-len": [1, 120, 2],
    "missing-expect": 0,
    "jasmine/no-global-setup":	2,
    "jasmine/no-focused-tests": 2,
    "jasmine/no-disabled-tests": 1,
    "no-use-before-define": ["warn"],
    "no-param-reassign": 0,
    "strict": 0,
    "import/no-extraneous-dependencies": 0,
    "prefer-spread": 0,
    "prefer-rest-params": 0,
    "prefer-destructuring": 0
  },
  "ecmaFeatures": {
    "modules":true,
    "arrowFunctions":true,
    "classes":true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "jquery": true,
    "node": true
  }
}